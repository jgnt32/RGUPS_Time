<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/ru.rgups.time" >
    
    <item android:state_pressed="true" android:color="@color/white" />
    
    <item android:state_enabled="true"
     	android:state_focused="true" android:color="@color/white" />
    
    <item android:state_enabled="true"
     	android:state_selected="true" android:color="@color/white" />
    
    <item android:state_activated="true" android:color="@color/white"></item>
    
    <item android:state_checked="true" android:color="@color/white"></item>
    
    <item android:state_checked="false" android:color="@color/white"></item>
    
    <item android:state_activated="false" android:color="@color/white"></item>

    <item android:color="@android:color/transparent" />
</selector>
